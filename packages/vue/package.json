{
  "name": "@zrook/vue-transitions",
  "version": "0.0.1",
  "description": "Vue transition animation library",
  "main": "dist/es/index.mjs",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "vite build && esno ./scripts/copy.ts",
    "release": "npm publish --access public"
  },
  "author": "zhaogongchengsi <zzhhaon@163.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/zhaogongchengsi",
  "homepage": "https://github.com/zhaogongchengsi/vue-transitions",
  "bugs": "https://github.com/zhaogongchengsi/vue-transitions/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaogongchengsi/vue-transitions.git"
  },
  "keywords": [
    "vue-transitions",
    "vue",
    "transition",
    "animation"
  ],
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.ts",
  "exports": {
    "./style": "./dist/es/style.css",
    ".": {
      "require": "./dist/lib/index.mjs",
      "import": "./dist/es/index.mjs"
    },
    "./fade": {
      "require": "./dist/lib/fade.js",
      "import": "./dist/es/fade.mjs"
    }
  },
  "sideEffects": [
    "dist/*",
    "*.css"
  ],
  "dependencies": {
    "@zrook/transitions": "workspace:^"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "typescript": "^5.1.3",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.3.4"
  }
}
