{
  "name": "@zrook/transitions",
  "version": "0.0.2",
  "description": "This is a library for animation switching with vue3 Transition api",
  "author": "zhaogongchengsi <zzhhaon@163.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/zhaogongchengsi",
  "homepage": "https://github.com/zhaogongchengsi/vue-transitions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaogongchengsi/vue-transitions.git"
  },
  "bugs": "https://github.com/zhaogongchengsi/vue-transitions/issues",
  "scripts": {
    "dev": "postcss src/index.css src/**/*.css -w --base src --dir dist",
    "build": "vt build -v ./vite.config.ts -c none",
    "build:css": "postcss src/index.css src/**/*.css --base src --dir dist",
    "prepublishOnly": "npm run build",
    "release": "npm publish --access public"
  },
  "keywords": [
    "transitions"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.css",
    "./fade": "./dist/fade/index.css",
    "./move": "./dist/move/index.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "devDependencies": {
    "@zrook/vue-transitions-fade": "workspace:^",
    "@zrook/vue-transitions-move": "workspace:^"
  }
}
